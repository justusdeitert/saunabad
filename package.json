{
  "name": "eleventy-sample",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:11ty": "eleventy --serve --config=eleventy.config.ts",
    "dev:postcss": "postcss ./src/styles/main.scss -o ./dist/styles/main.css --watch --verbose --config postcss.config.ts",
    "dev:esbuild": "esbuild src/scripts/main.ts --bundle --watch --sourcemap --outfile=./dist/scripts/main.js",
    "dev": "rm -rf ./dist && run-p dev:*",
    "build:11ty": "NODE_ENV=production eleventy --config=eleventy.config.ts",
    "build:postcss": "NODE_ENV=production postcss ./src/styles/main.scss -o ./dist/styles/main.css --verbose --config postcss.config.ts",
    "build:esbuild": "esbuild src/scripts/main.ts --bundle --minify --outfile=./dist/scripts/main.js",
    "build": "rm -rf ./dist && run-p build:*",
    "serve": "serve ./dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.5",
    "@csstools/postcss-sass": "^5.1.1",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "esbuild": "^0.27.2",
    "handlebars": "^4.7.8",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@dogstudio/highway": "^2.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "alpinejs": "^3.15.3",
    "global": "^4.4.0",
    "gsap": "^3.14.2",
    "postcss-import": "^16.1.1",
    "swiper": "^11.2.8"
  }
}
